  105  const {app, BrowserWindow} = require('electron')
  106  const path = require('path')
  107  const url = require('url')
  108  // Keep a global reference of the window object, if you don't, the window will
  109  // be closed automatically when the JavaScript object is garbage collected.
  110  let win
  111  function createWindow () {
  112    // Create the browser window.
  113    win = new BrowserWindow({width: 800, height: 600})
  114    // and load the index.html of the app.
  115    win.loadURL(url.format({
  116      pathname: path.join(__dirname, 'index.html'),
  117      protocol: 'file:',
  118      slashes: true
  119    }))
  120    // Open the DevTools.
  121    win.webContents.openDevTools()
  122    // Emitted when the window is closed.
  123    win.on('closed', () => {
  124      // Dereference the window object, usually you would store windows
  125      // in an array if your app supports multi windows, this is the time
  126      // when you should delete the corresponding element.
  127      win = null
  128    })
  129  }
